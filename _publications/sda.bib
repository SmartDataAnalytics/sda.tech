@PhdThesis{tramp-s-2014-thesis,
  Title                    = {Distributed {S}emantic {S}ocial {N}etworks: {A}rchitecture, {P}rotocols and {A}pplications},
  Author                   = {Sebastian Tramp},
  School                   = {University of Leipzig},
  Year                     = {2014},

  Address                  = {Leipzig, Germany},
