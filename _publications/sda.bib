@InProceedings{defactorest,
  Title                    = {De{F}acto - {A} {M}ultilingual {F}act {V}alidation {I}nterface},
  Author                   = {Ren{\'e} Speck and Diego Esteves and Jens Lehmann and Axel-Cyrille {Ngonga Ngomo}},
